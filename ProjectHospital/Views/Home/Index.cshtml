<section class="medical-departments-area" style="background-color: #f9f9f9; padding: 50px 0;">
    <div class="container">
        <div class="sec-title text-center" style="margin-bottom: 40px;">
            <h1 style="font-size: 32px; color: #333; font-weight: bold;">Các Bệnh Viện</h1>
            <span class="border" style="display: inline-block; width: 60px; height: 3px; background-color: #007bff; margin-top: 10px;"></span>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="medical-departments-carousel owl-carousel owl-theme">
                    @foreach (var a in ViewBag.list)
                    {
                        <div class="single-item text-center" style="display: flex; flex-direction: column; justify-content: space-between; align-items: center; height: 400px; padding: 20px; background-color: #fff; border-radius: 10px; box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1); transition: transform 0.3s ease, box-shadow 0.3s ease;">
                            <div class="iocn-holder" style="width: 100%; max-width: 250px; height: 250px; display: flex; justify-content: center; align-items: center; overflow: hidden; border-radius: 10px; background-color: #f0f0f0; transition: transform 0.3s ease;">
                                <img src="~/Areas/Admin/images/@a[5]" alt="Cardiology Image" style="width: 100%; height: 100%; object-fit: cover; transition: transform 0.3s ease;">
                            </div>
                            <div class="text-holder" style="flex-grow: 1; overflow: hidden; text-align: center; margin-top: 20px;">
                                <h3 style="font-size: 18px; color: #333; font-weight: 600; margin-bottom: 5px;">@a[1]</h3>
                                <p style="font-size: 14px; color: #666; margin: 0; line-height: 1.4; height: 50px; overflow: hidden;">@a[2]</p>
                            </div>
                            <a class="readmore" href="~/Department/HienThiBacSi?id=@a[0]" style="padding: 10px 20px; background-color: #007bff; color: #fff; text-decoration: none; border-radius: 5px; margin-top: 20px; transition: background-color 0.3s ease, transform 0.3s ease;">
                                Đọc thêm
                            </a>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="sec-title text-center" style="margin: 40px;">
            <h1 style="font-size: 32px; color: #333; font-weight: bold;">Các Bác Sĩ</h1>
            <span class="border" style="display: inline-block; width: 60px; height: 3px; background-color: #007bff; margin-top: 10px;"></span>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="medical-departments-carousel owl-carousel owl-theme">
                    @foreach (var b in ViewBag.listBS)
                    {
                        <div class="single-item text-center" style="display: flex; flex-direction: column; justify-content: space-between; align-items: center; height: 400px; padding: 20px; background-color: #fff; border-radius: 10px; box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1); transition: transform 0.3s ease, box-shadow 0.3s ease;">
                            <div class="iocn-holder" style="width: 100%; max-width: 175px; height: 1050px; display: flex; justify-content: center; align-items: center; overflow: hidden; border-radius: 10px; background-color: #f0f0f0; transition: transform 0.3s ease;">
                                <img src="~/Hinh/team/@b[7]" alt="Cardiology Image" style="width: 100%; height: 100%; object-fit: cover; transition: transform 0.3s ease;">
                            </div>
                            <div class="text-holder" style="flex-grow: 1; overflow: hidden; text-align: center; margin-top: 20px;">
                                <h3 style="font-size: 18px; color: #333; font-weight: 600; margin-bottom: 5px;">@b[1]</h3>
                                <p style="font-size: 14px; color: #666; margin: 0; line-height: 1.4; height: 50px; overflow: hidden;">@b[4]</p>
                                <p style="font-size: 14px; padding-bottom:50px; color: #666; margin: 0; line-height: 1.4; height: 50px; overflow: hidden;">@b[5]</p>
                            </div>

                            <a class="readmore" href="" style="padding: 10px 20px; background-color: #007bff; color: #fff; text-decoration: none; border-radius: 5px; margin-top: 20px; transition: background-color 0.3s ease, transform 0.3s ease;">
                                Đọc thêm
                            </a>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>




