<!-- Khởi tạo Owl Carousel -->
<script>
    $(document).ready(function () {
        $(".medical-departments-carousel").owlCarousel({
            items: 4, // Hiển thị 4 item cùng lúc
            loop: true, // Bật vòng lặp
            margin: 10, // Khoảng cách giữa các item
            nav: true, // Hiển thị nút điều hướng
            navText: ["<span class='fa fa-angle-left'></span>", "<span class='fa fa-angle-right'></span>"], // Nút điều hướng
            autoplay: true, // Tự động chuyển đổi
            autoplayTimeout: 5000, // Thời gian mỗi lần chuyển
        });
    });
</script>


